<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <fragment
        android:id="@+id/eleccion"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="es.iesnervion.diana.a2examen2eval.Fragments.FragmentListado"
        tools:layout="@layout/fragment_listado"/>

    <fragment
        android:id="@+id/insertar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="es.iesnervion.diana.a2examen2eval.Fragments.FragmentInsertar"
        tools:layout="@layout/fragment_insercion"/>



</androidx.constraintlayout.widget.ConstraintLayout>